# SDTM Pipeline Environment Variables
# Copy this file to .env and fill in your values

# Anthropic API Key (required)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Anthropic Model (optional - defaults to claude-opus-4-6)
ANTHROPIC_MODEL=claude-opus-4-6

# Max tokens per response (optional - defaults to 16000)
ANTHROPIC_MAX_TOKENS=16000

# Neo4j Database (optional)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password

# AWS Credentials (optional - for S3 access)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

# LangGraph Settings
LANGGRAPH_DEFAULT_RECURSION_LIMIT=250
LANGGRAPH_RECURSION_LIMIT=250

# File Server Port
FILE_SERVER_PORT=8090

# LangSmith (optional - for tracing)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your-langsmith-api-key
LANGCHAIN_PROJECT=sdtm-pipeline

# Tavily API Key (optional - for web search)
TAVILY_API_KEY=your-tavily-api-key

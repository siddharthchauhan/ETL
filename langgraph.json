{
  "dockerfile_lines": [
    "RUN apt-get update && apt-get install -y --no-install-recommends build-essential gcc g++ libffi-dev libxml2-dev libxslt-dev && rm -rf /var/lib/apt/lists/*"
  ],
  "dependencies": ["."],
  "graphs": {
    "sdtm_agent": {
      "path": "sdtm_pipeline.langgraph_chat.graph:agent",
      "config": {
        "recursion_limit": 250
      }
    },
    "sdtm_deepagent": {
      "path": "sdtm_pipeline.deepagents.graph:graph",
      "config": {
        "recursion_limit": 250
      }
    },
    "sdtm_production": {
      "path": "sdtm_pipeline.deepagents.production_graph:graph",
      "config": {
        "recursion_limit": 250
      }
    },
    "sdtm_chat": {
      "path": "sdtm_pipeline.langgraph_chat.graph:agent",
      "config": {
        "recursion_limit": 250
      }
    }
  },
  "env": ".env.example"
}
